<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="dialogPopButtonConfirm">
        <!--<item name="android:background">@drawable/sel_base_dialog_button_confirm</item>-->
        <item name="android:textColor">@color/col_33</item>
        <item name="android:textSize">19dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="baseButton">
        <!--<item name="android:background">@color/theme_bg</item>-->
        <item name="android:textColor">@color/col_33</item>
        <item name="android:textSize">16dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
    </style>


    <attr name="autoScaleTextViewStyle" format="reference" />
    <declare-styleable name="AutoScaleTextView">
        <attr name="minTextSize" format="dimension" />
    </declare-styleable>


</resources>
